.panel-tools-margins {
    right: 70px !important;
}
.select-check {
    margin-bottom: 0;
    margin-top: 0;
    padding-left: 0;
}

.select-check label {
    margin-right: 0;
    /*padding-left: 10px !important;*/
}

.margin-bottom-100 {
    margin-bottom:100px !important;
}

.fix-width {
    width:10px;
}

.no-border {
    border:none !important;
}

.btn[disabled] {
    pointer-events: none !important;
}

.sortorder:before {
    content: "\f0d7";
}

.sortorder.reverse:before {
    content: "\f0d8";
}

.footer-fixed {
    position:fixed;
    background-color:#f9f9f9 !important;
}

.required:before {
    content:"*";
    display:inline;
    color:#E6674A;
}

.pagination {
    margin: 0 !important;
}
